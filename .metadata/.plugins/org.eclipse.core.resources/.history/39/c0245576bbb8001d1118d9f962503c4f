package sample.work1;

import java.util.Scanner;

public class Test01 {

	public static void main(String[] args) {
		
		Scanner sc1 = new Scanner(System.in);
		int num = 1;
		int tg = 10;
		while (num == tg) {
			if(num == tg) {
				System.out.println(num);
				System.out.println("목표 달성");
				break;}
			
			System.out.println("1을 더합니다.");
			num++;
			System.out.println(num);
			continue;
			
			
			}
		}
	}


