package edu.java.loop03;

public class LoopMain03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// 1. 구구단 출력
		
		for ( int i = 1; i <= 9 ; i++) {
			System.out.println("2 x " + i + " = " + 2*i);
		
	}
	for (int i = 1; i <=100 ; i++) {
		System.out.print(i + " ");
		for(int j = 1; j <= 100 ; j*=10) {
			
		}
		}
	}
}
